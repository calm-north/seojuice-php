{
    "name": "seojuice/seojuice",
    "description": "Official PHP SDK for the SEOJuice Intelligence API",
    "type": "library",
    "license": "MIT",
    "keywords": ["seo", "analytics", "seojuice", "api", "sdk", "internal-links", "marketing"],
    "homepage": "https://seojuice.com",
    "authors": [
        {
            "name": "Vadim Kravcenko",
            "email": "vadim@seojuice.io"
        },
        {
            "name": "Calm North Labs UAB"
        }
    ],
    "support": {
        "source": "https://github.com/calm-north/seojuice-php",
        "issues": "https://github.com/calm-north/seojuice-php/issues"
    },
    "suggest": {
        "predis/predis": "For Redis-based caching of SEO suggestions",
        "psr/http-server-middleware": "For PSR-15 middleware integration"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SEOJuice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SEOJuice\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
